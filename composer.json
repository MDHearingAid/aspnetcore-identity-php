{
    "name": "mdhearing/aspnetcore-identity",
    "type": "library",
    "description": "A direct port of the algorithms used to generate and verify ASP.NET password hashes in .NET Core",
    "keywords": [
        "MDHearingAid",
        "aspnetcore-identity"
    ],
    "homepage": "https://github.com/MDHearingAid/aspnetcore-identity",
    "license": "MIT",
    "authors": [
        {
            "name": "Steven Maguire",
            "email": "it@mdhearingaid.com",
            "homepage": "https://github.com/MDHearingAid",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2 || ~8.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MDHearing\\AspNetCore\\Identity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MDHearing\\AspNetCore\\Identity\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs --standard=psr2 src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
